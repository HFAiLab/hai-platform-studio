  },
  "todohighlight.keywords": [
    "REVIEW:",
    {
      "text": "NOTE",
      "color": "#ffffff",
      "backgroundColor": "green",
      "overviewRulerColor": "grey"
    },
    {
      "text": "@hfchange",
      "color": "#ffffff",
